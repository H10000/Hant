            <label class="layui-form-label" style="width:auto;">组织名称</label>
            <div class="layui-input-inline" style="width:300px;">
                <select name="SearchUserName" xm-select="select_SearchParentOuID" xm-select-search="" xm-select-search-type="dl" xm-select-radio> </select>
            </div>
        </div>
        <div class="layui-inline">
            <div class="layui-input-inline" style="padding: 0 0 10px;margin-left:20px;">
                <input type="checkbox" name="SearchStatus" lay-skin="switch" lay-text="启用|禁用" checked>
            </div>
        </div>
        <div class="layui-inline" style="margin-left:20px;">
            <button lay-submit lay-filter="SearchButton" class="layui-btn">搜索</button>
        </div>
    </div>
    <div class="layui-card-body" style="overflow:auto;">
        <table id="table1" lay-filter="table1"></table>
